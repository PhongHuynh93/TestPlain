<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="dhbk.android.plaidapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Plaid">
        <activity
            android:name="io.plaidapp.ui.HomeActivity"
            android:theme="@style/Plaid.Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name="io.plaidapp.ui.SearchActivity"
            android:theme="@style/Plaid.Translucent.Search">
        </activity>
        <activity
            android:name="io.plaidapp.ui.SearchActivity2"
            android:label="@string/title_activity_search2"
            android:theme="@style/Plaid.Translucent.Search">
        </activity>
    </application>

</manifest>